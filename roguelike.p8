-- init --

function _init()
    update_func=update_game
    draw_func=draw_game
    start_game()

    -- globals --
    frame_timer=0
    plr_anim={240,241,242,243} --indexes for plr sprite
    x_direction={-1,1,0,0}
    y_direction={0,0,-1,1}
end

function start_game()
    input_buffer=-1
    plr_x=1
    plr_y=1
    plr_offset_x=0
    plr_offset_y=0
    plr_ini_offset_x=0 --initial offsets will keep animation from skipping for a frame
    plr_ini_offset_y=0
    plr_flip=false
    plr_anim_func=nil --function ptr for loading animations
    plr_timer=0

    window={}

    add_window(32,64,64,24,{"hello world!","how are you?"})
end

