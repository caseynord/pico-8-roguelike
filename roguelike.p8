-- ui --

function add_window(_x,_y,_w,_h,_text)
    local _window={x=_x,y=_y,    
                   w=_w,h=_h,    
                   text=_text}
    add(window,_window)
    return _window
end

function draw_window()
    for w in all(window) do
        local _wx,_wy,_ww,_wh=w.x,w.y,w.w,w.h
        rectfill2(_wx,_wy,_ww,_wh,0)
        rectfill2(_wx+1,_wy+1,_ww-2,_wh-2,6)
        rectfill2(_wx+2,_wy+2,_ww-4,_wh-4,0)
        _wx+=4
        _wy+=4
        clip(_wx,_wy,_ww-8,_wh-8)
        for i=1,#w.text do
            local _text=w.text[i]
                print(_text,_wx,_wy,6)
                _wy+=6
        end
    end
end

