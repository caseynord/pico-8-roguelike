-- mobs --

function add_mob(_type,_mx,_my)
    local _mob={
        x=_mx,
        y=_my,
        offset_x=0,
        offset_y=0,
        ini_offset_x=0, --initial offsets will keep animation from skipping for a frame
        ini_offset_y=0,
        flp=false,
        anim_func=nil, --function ptr for loading animations
        anim={}
    }
    for i=0,3 do
        add(_mob.anim,mob_anim[_type]+i)
    end
    add(mob,_mob)

    return _mob
end

