-- init --

function _init()
    _update_func=update_game
    _draw_func=draw_game
    start_game()
end

-->8
-- update --

function _update()
    _update_func()
end

function update_game()
    if btnp(⬅️) then
        plr_x-=1
    end
    if btnp(➡️) then
        plr_x+=1
    end
    if btnp(⬆️) then
        plr_y-=1
    end
    if btnp(⬇️) then
        plr_y+=1
    end
end

function update_game_over()

end

-->8
-- draw --

function _draw()
    _draw_func()
end

function draw_game()
    cls()
    map()
    
    palt(0,false) --set black to non transparent
    pal(7,10) --change pallete color 6 to 10
    spr(240,plr_x*8,plr_y*8) --draw layer
    pal() --reset pallete
end

function draw_game_over()

end

-->8
-- functions--

function start_game()
    plr_x=1
    plr_y=3
end



