-- draw --

function _draw()
    draw_func()
    draw_window() --called here so that it can be used anywhere in the game
end

function draw_game()
    cls()
    map()

    --draw player
    -- draw_sprite(get_frame(plr_anim),plr_x*8+plr_offset_x,plr_y*8+plr_offset_y,10,plr_flip)

    --draw monsters
    for m in all(mob) do
        draw_sprite(get_frame(m.anim),m.x*8+m.offset_x,m.y*8+m.offset_y,10,m.flp)
    end
end

function draw_game_over()
    --todo
end

function draw_sprite(_spr,_x,_y,_col,_flip)
    palt(0,false) --set black to non transparent
    pal(6,_col) --change pallete color 6 to 10
    spr(_spr,_x,_y,1,1,_flip) --draw layer
    pal() --reset pallete
end

